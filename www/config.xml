<?xml version='1.0' encoding='utf-8'?>
<widget id="com.aplicativo.uneb" version="1.3.17" versionCode="17" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
